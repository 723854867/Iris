<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.busap.vcs.data.mapper.UserRoleDao">

	<select id="select" resultType="com.busap.vcs.data.entity.UserRole" parameterType="com.busap.vcs.data.entity.UserRole">
		SELECT
		user,
		roles
		FROM
		user_role
		<where>
		<if test="user != null">
			AND user = #{user}
		</if>
		<if test="roles != null">
			AND roles = #{roles}
		</if>
		</where>
    </select>

</mapper>