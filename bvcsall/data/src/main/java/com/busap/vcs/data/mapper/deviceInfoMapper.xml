<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.busap.vcs.data.mapper.DeviceInfoDAO">
<sql id="page">limit #{pageStart},#{pageSize}</sql>
	<insert id="insert">
		insert into
		
		device_info(android_id,bt_mac,cpu,factory,iccid,imei,mac,meid,memory,model,operator,os_version,pixel,platform,pseudo_uniqe_id,screen_resolution,uid,unique_mark,uuid,wlan_mac)
		
		values(#{androidId},#{btMac},#{cpu},#{factory},#{iccid},#{imei},#{mac},#{meid},#{memory},#{model},#{operator},#{osVersion},#{pixel},#{platform},#{pseudoUniqeId},#{screenResolution},#{uid},#{uniqueMark},#{uuid},#{wlanMac})
	</insert>

	<select id="selectUuidByUniqueMark" resultType="String">
		select uuid from device_info where unique_mark=#{uniqueMark}
	</select>
	
	<update id="bindUidAndUuid">
		update device_info set uid=#{uid} where uuid=#{uuid}
	</update>
	
	
</mapper>